<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="720"
             x:Class="UserProtobufEFCore.AddUserView">
	<Border CornerRadius="0, 20, 20, 0" Background="#2A3447">
		<StackPanel>
			<TextBlock Text="ВВЕДИТЕ ИМЯ" FontSize="16" Foreground="WhiteSmoke" FontWeight="DemiBold" HorizontalAlignment="Center" Margin="0,100,0,15"/>
            <TextBox Width=" 400" Height="40" CornerRadius="10,10,10,10" Background="#2A3447" BorderBrush="WhiteSmoke"/>
			
			<TextBlock Text="ВВЕДИТЕ ФАМИЛИЮ" FontSize="16" Foreground="WhiteSmoke" FontWeight="DemiBold" HorizontalAlignment="Center" Margin="0,15,0,15" />
			<TextBox Width=" 400" Height="40" CornerRadius="10,10,10,10" Background="#2A3447" BorderBrush="WhiteSmoke"/>
			
			<TextBlock Text="ВВЕДИТЕ ТЕЛЕФОН" FontSize="16" Foreground="WhiteSmoke" FontWeight="DemiBold" HorizontalAlignment="Center" Margin="0,15,0,15"/>
			<TextBox Width=" 400" Height="40" CornerRadius="10,10,10,10" Background="#2A3447" BorderBrush="WhiteSmoke"/>
			
			<TextBlock Text="ВВЕДИТЕ EMAIL" FontSize="16" Foreground="WhiteSmoke" FontWeight="DemiBold" HorizontalAlignment="Center" Margin="0,15,0,15"/>
			<TextBox Width=" 400" Height="40" CornerRadius="10,10,10,10" Background="#2A3447" BorderBrush="WhiteSmoke"/>
			
		</StackPanel>
	</Border>
</UserControl>
